{
  "extends": "eslint:recommended",
  "env": {
    "es2022": true,
    "node": true,
    "jest": true,
    "mocha": true,
    "npm_config_cache": ".npmcache",
    "npm_config_prefer_offline": true
  },
  "globals": {
    "SELECT": true,
    "INSERT": true,
    "UPSERT": true,
    "UPDATE": true,
    "DELETE": true,
    "CREATE": true,
    "DROP": true,
    "CDL": true,
    "CQL": true,
    "CXL": true,
    "cds": true
  },
  "rules": {
    "no-console": "off",
    "require-atomic-updates": "off"
  }
}
